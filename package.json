{
  "name": "hugo-web-feeds",
  "version": "0.0.1",
  "private": true,
  "keywords": [
    "hug-web-feeds",
    "netlify",
    "websub"
  ],
  "description": "Hugo build helper for Netlify Functions",
  "main": "n/a",
  "scripts": {
    "postinstall": "npm run prebuild",
    "serve": "npm run start",
    "develop": "npm run start",
    "dev": "npm run start",
    "prestart": "npm run prebuild",
    "start": "netlify-lambda serve .netlify/functions/",
    "prebuild": "npm run clean; netlify-lambda install",
    "build": "netlify-lambda build .netlify/functions/",
    "postbuild": "NETLIFY_FUNCTIONS=${NETLIFY_FUNCTIONS:-.netlify/functions}; mkdir -p ../../${NETLIFY_FUNCTIONS}; cp -nv .netlify/build/functions/*.js ../../${NETLIFY_FUNCTIONS}; exit 0",
    "clean": "rimraf .netlify/build/",
    "cleanup": "npm run clean"
  },
  "author": "Julian Pawlowski <julian.pawlowski@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.1.0",
    "netlify-lambda": "^1.6.3",
    "request": "^2.88.0",
    "rimraf": "^3.0.0"
  }
}
